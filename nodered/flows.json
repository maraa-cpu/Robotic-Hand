    {
        "id": "crea_stringa_manuale",
        "type": "function",
        "z": "75a0802cafec364d",
        "name": "Raccogli Dita e Formatta",
        "func": "// Salva l'ultimo valore di ogni dito\nvar dita = context.get('dita') || {pollice:0, indice:0, medio:0, anulare:0, mignolo:0};\ndita[msg.topic] = msg.payload;\ncontext.set('dita', dita);\n\n// Controlla la modalit√†\nvar modalita = flow.get('modalita_corrente') || \"manuale\";\n\nif (modalita === \"manuale\") {\n    // Creiamo la stringa con i 5 angoli\n    msg.payload = dita.pollice + \",\" + dita.indice + \",\" + dita.medio + \",\" + dita.anulare + \",\" + dita.mignolo;\n    \n    // FORZIAMO IL TOPIC CORRETTO per MQTT!\n    msg.topic = \"robot/mano/motori\";\n    \n    return msg;\n}\n\nreturn null;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 890,
        "y": 840,
        "wires": [
