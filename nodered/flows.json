    {
        "id": "mano_grafica_ui_originale",
        "type": "ui_template",
        "z": "75a0802cafec364d",
        "group": "gruppo_visualizzazione",
        "name": "Mano Animata",
        "order": 1,
        "width": 6,
        "height": 6,
        "format": "<div style=\"display:flex; justify-content:center; align-items:center; width:100%; height:100%;\">\n    <svg width=\"200\" height=\"200\" viewBox=\"0 0 200 200\">\n        <line x1=\"45\" y1=\"130\" ng-attr-x2=\"{{tX}}\" ng-attr-y2=\"{{tY}}\" stroke=\"#fadcbc\" stroke-width=\"18\" stroke-linecap=\"round\"/>\n        <line x1=\"75\" y1=\"120\" x2=\"75\" ng-attr-y2=\"{{iY}}\" stroke=\"#fadcbc\" stroke-width=\"18\" stroke-linecap=\"round\"/>\n        <line x1=\"100\" y1=\"120\" x2=\"100\" ng-attr-y2=\"{{mY}}\" stroke=\"#fadcbc\" stroke-width=\"18\" stroke-linecap=\"round\"/>\n        <line x1=\"125\" y1=\"120\" x2=\"125\" ng-attr-y2=\"{{rY}}\" stroke=\"#fadcbc\" stroke-width=\"18\" stroke-linecap=\"round\"/>\n        <line x1=\"150\" y1=\"130\" ng-attr-x2=\"{{pX}}\" ng-attr-y2=\"{{pY}}\" stroke=\"#fadcbc\" stroke-width=\"18\" stroke-linecap=\"round\"/>\n        <path d=\"M 35,120 Q 30,190 60,200 L 140,200 Q 170,190 165,120 Z\" fill=\"#fadcbc\" stroke=\"none\"/>\n        <path d=\"M 35,120 Q 30,190 60,200 L 140,200 Q 170,190 165,120 Z\" fill=\"none\" stroke=\"#333\" stroke-width=\"2\"/>\n    </svg>\n</div>\n<script>\n    (function(scope) {\n        scope.tX = 20; scope.tY = 80;\n        scope.iY = 40; scope.mY = 30; scope.rY = 40;\n        scope.pX = 180; scope.pY = 70;\n        scope.$watch('msg.payload', function(data) {\n            if (typeof data === 'string') {\n                var a = data.split(',');\n                if (a.length === 5) {\n                    var v0 = parseInt(a[0]) / 180;\n                    var v1 = parseInt(a[1]) / 180;\n                    var v2 = parseInt(a[2]) / 180;\n                    var v3 = parseInt(a[3]) / 180;\n                    var v4 = parseInt(a[4]) / 180;\n                    scope.tX = 20 + (v0 * 25);\n                    scope.tY = 80 + (v0 * 50);\n                    scope.iY = 40 + (v1 * 80);\n                    scope.mY = 30 + (v2 * 90);\n                    scope.rY = 40 + (v3 * 80);\n                    scope.pX = 180 - (v4 * 30);\n                    scope.pY = 70 + (v4 * 60);\n                }\n            }\n        });\n    })(scope);\n</script>",
